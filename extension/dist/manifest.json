{
  "manifest_version": 3,
  "name": "ChatGPT Extension",
  "version": "1.0",
  "description": "A Chrome extension to interact with ChatGPT.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "favicon.png",
      "48": "favicon.png",
      "128": "favicon.png"
    }
  },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
