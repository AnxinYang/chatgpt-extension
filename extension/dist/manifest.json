{
  "manifest_version": 3,
  "name": "ChatExtension - A ChatGPT Extension",
  "version": "1.0",
  "description": "A Chrome extension to interact with ChatGPT for All Websites.",
  "action": {},
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "icons": {
    "16": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  }
}
